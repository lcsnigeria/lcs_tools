{
    "name": "lcsng/tools",
    "description": "External library for the LCS platforms management and for others.",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "LCSNG\\Tools\\": "src/"
        }
    },
    "require": {
        "php": "^8.3",
        "vlucas/phpdotenv": "^5.6",
        "scssphp/scssphp": "^2.0",
        "symfony/error-handler": "^6.2",
        "minishlink/web-push": "^9.0",
        "web-token/jwt-library": "^4.0",
        "symfony/mailer": "^7.3",
        "symfony/http-client": "^7.3",
        "phpmailer/phpmailer": "^6.10",
        "mailgun/mailgun-php": "^4.3",
        "nyholm/psr7": "^1.8",
        "resend/resend-php": "^0.18.0",
        "matomo/device-detector": "^6.4"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "php-http/discovery": true
        }
    },
    "authors": [
